{
  "manifest_version": 3,
  "name": "My Pocket",
  "version": "1.0.0",
  "description": "Stored the information and access easily",
  "author": "Raju",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["*://*/*"]
}
